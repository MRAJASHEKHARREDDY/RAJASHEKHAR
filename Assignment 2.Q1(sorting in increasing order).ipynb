   "source": [
    "def last(n):\n",
    "    return n[-1]\n",
    "def sort(a):\n",
    "    return sorted(a, key = last)\n",
    "a = [(2,5),(1,2),(4,4),(2,3),(2,1)]\n",
    "print(\"sorted:\")\n",
    "print(sort(a))"
